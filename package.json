{
  "name": "dom-serialization",
  "version": "1.0.1",
  "description": "Serialize the DOM",
  "main": "dist/index.js",
  "keywords": [
    "dom",
    "serialization"
  ],
  "author": "Youngjin Jang",
  "repository": {
    "type": "git",
    "url": "https://github.com/swimyoung/dom-serialization"
  },
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "report": "codecov",
    "lint": "eslint .",
    "dev": "webpack-dev-server --config ./docs/webpack.config.js",
    "docs": "cross-env NODE_ENV=production webpack -p --config ./docs/webpack.config.js",
    "compile": "babel src -d dist --ignore '**/*.spec.js'",
    "bundle": "webpack -p",
    "dist": "rimraf dist && run-p compile bundle docs"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "codecov": "^3.2.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "jest-in-case": "^1.0.2",
    "mini-css-extract-plugin": "^0.7.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5"
  },
  "sideEffects": false,
  "prettier": {
    "singleQuote": true,
    "printWidth": 80,
    "trailingComma": "all"
  },
  "jest": {
    "testURL": "http://localhost",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  }
}
